{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "parser": "json",
        "tabWidth": 2,
        "useTabs": false,
        "trailingComma": "none"
      }
    }
  ]
}
